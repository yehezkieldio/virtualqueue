{
    "$schema": "https://turbo.build/schema.json",
    "ui": "stream",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [
                "build/**",
                ".vercel/**",
                "dist/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "typecheck": {
            "dependsOn": ["^typecheck"]
        },
        "db:push": {
            "cache": false
        },
        "db:studio": {
            "persistent": true,
            "cache": false
        },
        "db:generate": {
            "cache": false
        },
        "db:drop": {
            "cache": false
        },
        "db:migrate": {
            "cache": false
        },
        "start:web:dev": {
            "dependsOn": ["^build"],
            "cache": false,
            "persistent": true
        },
        "start:web": {
            "inputs": ["$TURBO_DEFAULT$", ".env"],
            "persistent": true
        },
        "start:api:dev": {
            "dependsOn": ["^build"],
            "cache": false,
            "persistent": true
        },
        "start:api": {
            "inputs": ["$TURBO_DEFAULT$", ".env"],
            "persistent": true
        },
        "//#format": {
            "cache": false
        },
        "//#lint": {
            "cache": false
        }
    },
    "globalDependencies": [".env"],
    "globalPassThroughEnv": ["NODE_ENV"]
}
